<h1>Airline Planner</h1>
<hr />
<h2>Arrivals:</h2>
@if (Model.Count != 0)
{
  <ul>
    @foreach (var arrival in Model)
    {
      <li><a href="/arrivals/@arrival.GetId()">@arrival.GetArrivalCityName()</a></li>
    }
  </ul>
}
<h4><a href="/arrival/new">Add New Arrival City</a></h4>
<h4><a href="/">Return home</a></h4>
